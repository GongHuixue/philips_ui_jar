<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

<item android:id="@android:id/background">
    <shape>
        

        <gradient
            android:angle="270"
            android:centerColor="#2e394b"
            android:centerY="0.75"
            android:endColor="#ff747674"
            android:startColor="#ff5a5d5a" />
    </shape>
</item>
<item android:id="@android:id/secondaryProgress">
    <clip>
        <shape>
           


            <gradient
                android:angle="270"
                android:centerColor="#455671"
                android:centerY="0.75"
                android:endColor="#455671"
                android:startColor="#455671" />
        </shape>
    </clip>
</item>
<item android:id="@android:id/progress">
    <clip>
        <shape>
            

            <gradient
                android:angle="270"
                android:endColor="#6ebffd"
                android:startColor="#6ebffd" />
        </shape>
    </clip>
</item>
</layer-list>