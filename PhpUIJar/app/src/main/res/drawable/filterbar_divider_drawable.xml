<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90">

    <shape android:shape="line">
        <size
            android:height="@dimen/filterbar_divider_height"
            android:width="@dimen/filterbar_divider_height" />
        <padding
            android:top="@dimen/filterbar_divider_margin_top_height"
            android:bottom="@dimen/filterbar_divider_margin_bottom_height" />

        <stroke
            android:width="1dp"
            android:color="#66FFFFFF" />

        <solid android:color="@android:color/transparent" />
    </shape>
</rotate>